<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="luckyBundle\Entity\Concert" repository-class="luckyBundle\Repository\Concert" table="concert">

        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="date" type="datetime" column="date" />
        <field name="place" type="string" column="place" />
        <field name="address" type="string" column="address" />

    </entity>
</doctrine-mapping>